{
  "name": "familytree-fullstack",
  "version": "1.0.0",
  "description": "Family Tree Management System - Full Stack Application",
  "main": "backend/server.js",
  "scripts": {
    "install": "cd backend && npm install && cd ../frontend && npm install",
    "install:backend": "cd backend && npm install",
    "install:frontend": "cd frontend && npm install",
    "dev:backend": "cd backend && npm run dev",
    "dev:frontend": "cd frontend && npm run dev",
    "dev": "concurrently \"npm run dev:backend\" \"npm run dev:frontend\"",
    "build": "cd frontend && npm run build",
    "start": "cd backend && npm start",
    "start:prod": "cd backend && cross-env NODE_ENV=production npm start",
    "deploy": "npm run build && npm run start:prod",
    "heroku-postbuild": "npm run build"
  },
  "keywords": [
    "family-tree",
    "genealogy",
    "react",
    "nodejs",
    "postgresql"
  ],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "cloudinary": "^2.9.0",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "dagre": "^0.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-rate-limit": "^7.1.5",
    "express-validator": "^7.0.1",
    "helmet": "^7.1.0",
    "jsonwebtoken": "^9.0.2",
    "mongoose": "^8.0.3",
    "multer": "^1.4.5-lts.1",
    "nodemailer": "^6.9.8",
    "pg": "^8.16.3",
    "sequelize": "^6.37.7",
    "stripe": "^14.10.0"
  },
  "devDependencies": {
    "concurrently": "^8.2.2",
    "cross-env": "^7.0.3"
  }
}
