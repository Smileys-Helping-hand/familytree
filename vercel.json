{
  "version": 2,
  "framework": "vite",
  "functions": {
    "api/index.js": {
      "includeFiles": "{backend/**,node_modules/pg/**,node_modules/pg-hstore/**,node_modules/sequelize/**,backend/node_modules/pg/**,backend/node_modules/pg-hstore/**,backend/node_modules/sequelize/**}"
    }
  },
  "installCommand": "npm install; npm install --prefix backend; npm install --prefix frontend",
  "buildCommand": "npm run build --prefix frontend",
  "outputDirectory": "frontend/dist",
  "rewrites": [
    {
      "source": "/api/(.*)",
      "destination": "/api"
    },
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ]
}
